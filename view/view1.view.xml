	<html:style>
		.bluetext{ color:blue; }
        .myimage{float:right !important; width:150px; height:100px;}
        .mylogo{float:left !important; width:150px; height:100px;}
	</html:style>
	<Image class="myimage" src="http://www.sap.com/global/ui/images/global/sap-logo.png"/>
